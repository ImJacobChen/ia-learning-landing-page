<!doctype html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">

    <!-- include system scripts, page meta, and header code injection -->
    {squarespace-headers}
  </head>
  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">

    <div class="site-container">

      <header class="site-header">
        <!-- site navigation -->
        <squarespace:navigation navigationId="siteNav" template="nav" />
      </header>

      <!-- cms content injection point -->
      <main class="content-container" role="main" data-content-field="main-content">
       {squarespace.main-content}

        <div class="see-more-block">
          <div class="see-more-block__left">
            <squarespace:block-field id="seeMoreBlock1Image" columns="1"/>
          </div><div class="see-more-block__right">
            <div class="see-more-block__right__title">
              <squarespace:block-field id="seeMoreBlock1Title" columns="1"/>
            </div>
            <button class="see-more-block__right__see-more-button">
              <squarespace:block-field id="seeMoreBlock1Button" columns="1"/>
            </button>
          </div>
        </div>
      </main>

      <!--Footer with open block field -->
      <footer class="site-footer">
        <squarespace:block-field id="footerBlocks" columns="12" />
      </footer>

    </div>

    <!-- combo and minify scripts when not logged in -->
    <squarespace:script src="site.js" combo="{.if authenticatedAccount}false{.or}true{.end}" />

    <!-- other scripts, and footer code injection -->
    {squarespace-footers}

  </body>
</html>
